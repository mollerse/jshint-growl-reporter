{
  "name": "jshint-growl-reporter",
  "version": "1.0.0",
  "description": "Growl reporter for JSHint",
  "keywords": [
    "jshint",
    "growl",
    "reporter"
  ],
  "author": {
    "name": "Stian Veum MÃ¸llersen",
    "email": "stian.moellersen@gmail.com",
    "url": "https://github.com/mollerse"
  },
  "repository": "mollerse/jshint-growl-reporter",
  "scripts": {
    "test": "istanbul test ./node_modules/mocha/bin/_mocha --report html -- test/*.js --reporter spec",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && rm -rf ./coverage"
  },
  "dependencies": {
    "growl": "~1.7.0",
    "lodash": "^3.9.3"
  },
  "devDependencies": {
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "istanbul": "*",
    "sinon": "~1.8.2",
    "chai": "~1.9.0",
    "require-subvert": "0.0.3"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
